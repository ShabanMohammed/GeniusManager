@page "/EducationLevel/{namePage}"
@page "/EducationLevel/{namePage}/{id:long}"
@using GeniusManager.App.Components.Pages.LookupPropery.GenericPages
@using GeniusManager.Domain.Entities.Lookups
@rendermode InteractiveServer

<PageTitle>المستوى التعليمى</PageTitle>
@if (namePage == "list")
{
    <GenericTable TEntity="EducationLevel" Title="المستوى التعليمى" />
}
else if ((namePage == "addNew"))
{

    <GenericAddNew TEntity="EducationLevel" Title="المستوى التعليمى" />
}
else if (namePage == "update")
{
    <GenericEdit TEntity="EducationLevel" Title="المستوى التعليمى" id="id" />
}
else if ((namePage == "deleteconfirm"))
{
    <GenericDeleteConfirm TEntity="EducationLevel" Title="المستوى التعليمى" id="id" />
}
@code {

    [Parameter] public string namePage { get; set; } = "list";
    [Parameter] public long id { get; set; }
}
