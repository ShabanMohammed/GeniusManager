@page "/appointmenttype/{namePage}"
@page "/appointmenttype/{namePage}/{id:long}"
@using GeniusManager.App.Components.Pages.LookupPropery.GenericPages
@using GeniusManager.Domain.Entities.Lookups
@rendermode InteractiveServer

<PageTitle>نوع التعيين</PageTitle>
@if (namePage == "list")
{
    <GenericTable TEntity="AppointmentType" Title="نوع التعيين" />
}
else if ((namePage == "addNew"))
{

    <GenericAddNew TEntity="AppointmentType" Title="نوع التعيين" />
}
else if (namePage == "update")
{
    <GenericEdit TEntity="AppointmentType" Title="نوع التعيين" id="id" />
}
else if ((namePage == "deleteconfirm"))
{
    <GenericDeleteConfirm TEntity="AppointmentType" Title="نوع التعيين" id="id" />
}
@code {

    [Parameter] public string namePage { get; set; } = "list";
    [Parameter] public long id { get; set; }
}
