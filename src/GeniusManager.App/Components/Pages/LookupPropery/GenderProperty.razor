@page "/gender/{namePage}"
@page "/gender/{namePage}/{id:long}"
@using GeniusManager.App.Components.Pages.LookupPropery.GenericPages
@using GeniusManager.Domain.Entities.Lookups
@rendermode InteractiveServer

<PageTitle>نوع الجنس</PageTitle>
@if (namePage == "list")
{
    <GenericTable TEntity="Gender" Title="نوع الجنس"/>
}
else if ((namePage == "addNew"))
{
    
    <GenericAddNew TEntity="Gender" Title="نوع الجنس" />
}
else if (namePage=="update")
{
    <GenericEdit TEntity="Gender" Title="نوع الجنس" id="id"/>
}
else if ((namePage == "deleteconfirm"))
{
    <GenericDeleteConfirm TEntity="Gender" Title="نوع الجنس" id="id" />
}
@code {

    [Parameter] public string namePage { get; set; } = "list";
    [Parameter] public long id { get; set; }
}
